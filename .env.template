# SLBrowser Environment Variables Template
# Copy this file to .env and fill in your values

# Google Gemini API Key (required)
# Get from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_api_key_here

# Optional: Logging configuration
SLBROWSER_LOG_LEVEL=INFO

# Optional: Request timeouts (seconds)
SLBROWSER_REQUEST_TIMEOUT=30
SLBROWSER_AI_TIMEOUT=30

# Optional: Search and analysis limits
SLBROWSER_MAX_SEARCH_RESULTS=10
SLBROWSER_MAX_CONTENT_LENGTH=15000
